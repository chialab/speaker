{
  "name": "@chialab/speaker",
  "version": "1.2.0",
  "type": "module",
  "description": "A text2speech implementation for HTML documents.",
  "types": "types/index.d.ts",
  "module": "dist/esm/speaker.js",
  "main": "dist/cjs/speaker.cjs",
  "exports": {
    ".": {
      "import": "./dist/esm/speaker.js",
      "require": "./dist/cjs/speaker.cjs",
      "default": "./dist/speaker.js"
    },
    "./dist/*": "./dist/*"
  },
  "files": [
    "package.json",
    "dist",
    "types",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "types": "rimraf types && tsc --declaration --emitDeclarationOnly --declarationDir ./types",
    "build": "rimraf dist && yarn build:esm && yarn build:cjs",
    "build:esm": "rna src/index.ts --output dist/esm/speaker.js --minify --format esm",
    "build:cjs": "rna src/index.ts --output dist/cjs/speaker.cjs --minify --format cjs",
    "lint": "eslint --ext .ts src --cache",
    "prepack": "yarn types && yarn build"
  },
  "license": "MIT",
  "author": "Chialab <dev@chialab.io> (https://www.chialab.it)",
  "dependencies": {
    "@chialab/proteins": "^3.2.6",
    "@chialab/text-helpers": "^2.2.12"
  },
  "devDependencies": {
    "@chialab/eslint-config": "^3.0.0",
    "@chialab/rna": "^0.12.2",
    "@chialab/rna-bundler": "^0.12.30",
    "eslint": "^7.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  }
}
